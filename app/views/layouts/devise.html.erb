<!DOCTYPE html>
<html>
  <head>
    <title>Cocktail Party</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <!-- Bootstrap core CSS -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet">
    <!-- Material Design Bootstrap -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.5.4/css/mdb.min.css" rel="stylesheet">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
 
  <body>
    <nav class="navbar navbar-dark black">

      <%# <a class="navbar-brand" href="#"> %>
      					<img id="logo"src="../assets/side-view-logo.png" width="30%", alt="logo" >

       <%#= image_tag 'cocktail-party.png', height:'120x120', alt: "logo" %>
      <%# </a> %>

     </nav> 


    <%# <nav class="navbar navbar-expand-lg black fixed-top scrolling-navbar">
				<div class="container">
					<img id="logo"src="/assets/side-view-logo.png" width="50%", alt="logo" >
				</div> %>
    <%= yield %>
    <!-- JQuery -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0/js/bootstrap.min.js"></script>
    <!-- MDB core JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.5.4/js/mdb.min.js"></script>
  </body>
</html>