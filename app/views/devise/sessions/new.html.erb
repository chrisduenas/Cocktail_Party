
<body class= "background-signing-in-up">
<div class="col-lg-4 offset-lg-4 col-sm-6 offset-sm-3 mt-4">

    <!-- Card content -->
    <div class="card-body"id= "card-form-in">
      <h4 class="card-title text-center" id="log-in-up-text">Log In</h4>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div id= "form-contain">
        <div class="md-form" id="row" >
          <%= f.email_field :email, class: "form-control",  placeholder:"Email" %>
     

        </div>
        <div class="md-form" id="row">
          <%= f.password_field :password,  class: "form-control", placeholder: "Password" %>
      

        </div>

        <div class="text-center mt-4">
          <%= f.submit "Log in", class: 'btn btn-orange' %>
        </div>
        </div>
      <% end %>
    </div>
 

  <div class="mt-4 text-center indigo-text">
    <%= render "devise/shared/links" %>
  </div>

</div>
</body>